{"\u7535\u5f71\u540d\u79f0": "\u6cf0\u5766\u5c3c\u514b\u53f7 Titanic", "\u5bfc\u6f14": "\u8a79\u59c6\u65af\u00b7\u5361\u6885\u9686", "\u4e3b\u6f14": "\u83b1\u6602\u7eb3\u591a\u00b7\u8fea\u5361\u666e\u91cc\u5965 \u51ef\u7279\u00b7\u6e29\u4e1d\u83b1\u7279 \u6bd4\u5229\u00b7\u8d5e\u6069 \u51ef\u897f\u00b7\u8d1d\u8328 \u5f17\u5170\u897f\u4e1d\u00b7\u8d39\u820d \u683c\u52b3\u745e\u4e9a\u00b7\u65af\u56fe\u5c14\u7279 \u6bd4\u5c14\u00b7\u5e15\u514b\u65af\u987f \u4f2f\u7eb3\u5fb7\u00b7\u5e0c\u5c14 \u5927\u536b\u00b7\u6c83\u7eb3 \u7ef4\u514b\u591a\u00b7\u52a0\u535a \u4e54\u7eb3\u68ee\u00b7\u6d77\u5fb7 \u82cf\u831c\u00b7\u7231\u7c73\u65af \u5218\u6613\u65af\u00b7\u963f\u4f2f\u5185\u897f \u5c3c\u53e4\u62c9\u65af\u00b7\u5361\u65af\u67ef\u6069 \u963f\u90a3\u6258\u5229\u00b7\u8428\u52a0\u6d1b\u7ef4\u5947 \u4e39\u5c3c\u00b7\u52aa\u9f50 \u6770\u68ee\u00b7\u8d1d\u745e \u4f0a\u4e07\u00b7\u65af\u56fe\u5c14\u7279 \u827e\u6069\u00b7\u683c\u62c9\u6cd5\u5fb7 \u4e54\u7eb3\u68ee\u00b7\u83f2\u5229\u666e\u65af \u9a6c\u514b\u00b7\u6797\u8d5b\u00b7\u67e5\u666e\u66fc \u7406\u67e5\u5fb7\u00b7\u683c\u62c9\u7ff0 \u4fdd\u7f57\u00b7\u5e03\u8d56\u7279\u5a01\u5c14 \u827e\u745e\u514b\u00b7\u5e03\u91cc\u767b \u590f\u6d1b\u7279\u00b7\u67e5\u987f \u535a\u7eb3\u5fb7\u00b7\u798f\u514b\u65af \u8fc8\u514b\u5c14\u00b7\u82f1\u585e\u6069 \u6cd5\u59ae\u00b7\u5e03\u96f7\u7279 \u9a6c\u4e01\u00b7\u8d3e\u7ef4\u65af \u7f57\u838e\u7433\u00b7\u827e\u5c14\u65af", "\u7c7b\u578b": "\u5267\u60c5\u7231\u60c5\u707e\u96be", "\u5236\u7247\u56fd\u5bb6": "\u7f8e\u56fd", "\u4e0a\u6620\u65f6\u95f4": "1998-04-03(\u4e2d\u56fd\u5927\u9646)", "\u7247\u957f": "194\u5206\u949f", "\u8bc4\u5206": "9.4"}